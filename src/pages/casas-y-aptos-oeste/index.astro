---
import PropertyCategoryLayout from '../../layouts/PropertyLayout/PropertyCategoryLayout.astro';
import properties from '../../data/properties.json';

const category = properties.categories.find(cat => cat.id === 'casas-y-aptos-oeste');

// Valores predeterminados por si no se encuentra la categoría
const title = category?.title || 'Casas en Panamá Oeste';
const categoryProperties = category?.properties || [];
---

<PropertyCategoryLayout 
  title={title}
  properties={categoryProperties}
/>
